{
  "name": "GeminiAPI_JS_Web",
  "base_url": "https://ai.google.dev/gemini-api/docs/quickstart",
  "include_patterns": [
    "https://ai.google.dev/gemini-api/docs/**",
    "https://ai.google.dev/api/**"
  ],
  "exclude_patterns": [
    "**/python/**",
    "**/android/**",
    "**/swift/**",
    "**/go/**",
    "**/dart/**"
  ],
  "output_dir": "output/gemini_js_skill/",
  "selectors": {
    "main_content": "article, main, .devsite-article-body",
    "title": "h1",
    "code_blocks": "pre, code, .devsite-code-buttons-container",
    "nav": "nav, .devsite-book-nav"
  }
}